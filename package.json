{
  "name": "full-react-app-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "@babel/register": "^7.4.0",
    "@material-ui/core": "^4.0.0-beta.1",
    "@material-ui/icons": "^4.0.0-beta.0",
    "deepmerge": "^3.2.0",
    "i18next": "^15.0.7",
    "node-sass": "^4.11.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-i18next": "^10.5.2",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "source-map-explorer": "^1.8.0",
    "styled-components": "^4.2.0"
  },
  "scripts": {
    "start": "npm-run-all generate-db start-mockapi start-no-db",
    "start-no-db": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "generate-db": "npx babel-node scripts/generateDB.js",
    "start-mockapi": "json-server --watch db.json --port 3001 &"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@storybook/addon-a11y": "^5.0.3",
    "@storybook/addon-actions": "^5.0.3",
    "@storybook/addon-console": "^1.1.0",
    "@storybook/addon-info": "^5.0.3",
    "@storybook/addon-knobs": "^5.0.3",
    "@storybook/addon-links": "^5.0.3",
    "@storybook/addon-storysource": "^5.0.3",
    "@storybook/addons": "^5.0.3",
    "@storybook/react": "^5.0.3",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.7.0",
    "chalk": "^2.4.2",
    "clsx": "^1.0.4",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.5",
    "faker": "^4.1.0",
    "json-schema-faker": "^0.5.0-rc16",
    "json-server": "^0.14.2",
    "npm-run-all": "^4.1.5",
    "react-app-rewire-workbox": "^2.0.1",
    "react-app-rewired": "^2.1.1",
    "storybook-addon-smart-knobs": "^4.1.0",
    "workbox-webpack-plugin": "^4.1.1"
  }
